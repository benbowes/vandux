{
  "name": "vandux",
  "version": "1.0.9",
  "description": "Vandux - VanillaJS Redux style pattern for use with standard html",
  "main": "dist/vandux.js",
  "scripts": {
    "start": "webpack --config webpack.config.prebuild.js && cross-env NODE_ENV=development webpack-dev-server",
    "build": "webpack --config webpack.config.prebuild.js && cross-env NODE_ENV=production webpack -p",
    "test": "webpack --config webpack.config.prebuild.js && mocha --require babel-register 'src/**/*__test__.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benbowes/vandux.git"
  },
  "keywords": [
    "vanilla",
    "js",
    "redux",
    "style",
    "store"
  ],
  "author": "bb@benbowes.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benbowes/vandux/issues"
  },
  "homepage": "https://github.com/benbowes/vandux#readme",
  "devDependencies": {
    "babel-core": "6.25.x",
    "babel-loader": "7.1.x",
    "babel-preset-es2015": "6.24.x",
    "babel-preset-stage-0": "6.24.x",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^7.0.2",
    "deep-freeze": "^0.0.1",
    "jsdom": "^11.5.1",
    "jsdom-simulant": "^1.1.2",
    "mocha": "^7.1.2",
    "ts-loader": "^7.0.4",
    "typescript": "^2.6.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
